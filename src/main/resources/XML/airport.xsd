<?xml version="1.0"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">

    <xs:element name="Models">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="Airport" maxOccurs="unbounded">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="Runway">
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:element name="LogicalRunway" maxOccurs="unbounded">
                                            <xs:complexType>
                                                <xs:sequence>
                                                    <xs:element name="direction" type="xs:string"/>
                                                    <xs:element name="TORA" type="xs:integer"/>
                                                    <xs:element name="TODA" type="xs:integer"/>
                                                    <xs:element name="ASDA" type="xs:integer"/>
                                                    <xs:element name="LDA" type="xs:integer"/>
                                                    <xs:element name="dispThresh" type="xs:integer"/>
                                                </xs:sequence>
                                                <xs:attribute name="degree" type="xs:string" use="required"/>
                                            </xs:complexType>
                                        </xs:element>
                                    </xs:sequence>
                                </xs:complexType>
                            </xs:element>
                        </xs:sequence>
                        <xs:attribute name="name" type="xs:string" use="required"/>
                        <xs:attribute name="code" type="xs:string" use="required"/>
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

</xs:schema>
